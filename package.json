{
  "name": "jroze",
  "version": "1.0.0",
  "description": "Jroze Port",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run start:prod || yarn run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn run client\"",
    "client": "cd client && yarn run start",
    "install": "cd client && yarn install",
    "build": "cd client && yarn build"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cheerio": "^1.0.0-rc.10",
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "faker": "^6.6.6",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.11",
    "nodemailer": "^6.7.3",
    "nodemon": "^2.0.15",
    "path": "^0.12.7",
    "twilio": "^3.76.1",
    "validator": "^13.7.0",
    "yarn": "^1.22.18"
  }
}
